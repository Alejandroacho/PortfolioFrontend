{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "noEmit": true,
    "include": [
      "./src/**/*.ts"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "exclude": [
      "node_modules"
    ],
    "typeRoots": [
      "node_modules/@types",
      "src/assets/types"
    ],
    "types": ["vite/client", "@types/node"]
  }
}